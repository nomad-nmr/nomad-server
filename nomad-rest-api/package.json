    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "filelist-utils": "^1.9.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "jszip": "^3.7.1",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nmr-load-save": "^0.17.2",
    "nodemailer": "^6.9.2",
    "openchemlib": "^8.3.0",
    "socket.io": "^4.5.4"
